{
  "name": "agentic-vulnerability-scanner",
  "version": "1.0.0",
  "description": "AI-Powered Agentic Vulnerability Scanner for source code analysis",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/cli/scanner-cli.js",
    "scan": "node src/cli/scanner-cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "build": "npm run lint && npm run test",
    "benchmark": "node src/evaluation/benchmark-runner.js"
  },
  "keywords": [
    "vulnerability",
    "security",
    "static-analysis",
    "ai",
    "agents",
    "java",
    "owasp"
  ],
  "author": "Cybersecurity Thesis Project",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "@langchain/anthropic": "^0.3.24",
    "@langchain/core": "^0.3.62",
    "@langchain/google-genai": "^0.2.14",
    "@langchain/openai": "^0.5.18",
    "commander": "^11.0.0",
    "eventemitter3": "^5.0.0",
    "fast-csv": "^4.3.0",
    "fast-glob": "^3.3.0",
    "langchain": "^0.3.29",
    "mime-types": "^2.1.0",
    "openai": "^4.0.0",
    "sqlite3": "^5.1.0",
    "xml2js": "^0.6.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "aavs": "./src/cli/scanner-cli.js"
  }
}
